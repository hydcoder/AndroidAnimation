<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	tools:context=".MainActivity"
	android:orientation="vertical">

	<Button
		android:id="@+id/vector_demo"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="@string/vector_demo" />

	<Button
		android:id="@+id/bezier_demo"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="@string/bezier_demo" />

	<Button
		android:id="@+id/path_measure_demo"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="@string/path_measure_demo" />
	<Button
		android:id="@+id/animation_demo"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="@string/animation_demo" />

</LinearLayout>